# .NET Core
bin/                          # Build outputs
obj/                          # Intermediate files
project.lock.json             # .NET Core lock file (if used)
project.fragment.lock.json    # Fragment lock file (if used)
artifacts/                     # Build artifacts
*.dll                          # Dynamic-link libraries
*.exe                          # Executable files
*.pdb                          # Debugging symbols
*.cache                        # Cache files
*.log                          # Log files

# Azure Functions
local.settings.json            # Local settings (e.g., connection strings)
.host/                         # Azure Functions host files
*.funcignore                   # Files to exclude from deployment package

# Visual Studio
.vs/                          # Visual Studio solution and project settings
.vs/**/                       # All subdirectories and files within .vs/

# VS Code
.vscode/                       # VS Code settings directory
.history/                      # VS Code local history

# Rider
.idea/                         # JetBrains Rider project settings

# User-specific files
*.suo                         # Solution user options
*.user                        # User-specific files
*.userosscache                # User-specific OS cache
*.sln.docstates               # Solution document states

# OS-specific files
.DS_Store                     # macOS Finder metadata
Thumbs.db                     # Windows thumbnail cache
Desktop.ini                   # Windows desktop settings

# Node.js
node_modules/                 # Node.js dependencies
npm-debug.log                 # NPM debug log
yarn-error.log                # Yarn error log

# JetBrains Rider
.idea/                         # Rider project settings (already included above)

# Mac System Files
.DS_Store                     # macOS Finder metadata

# Windows System Files
Thumbs.db                     # Windows thumbnail cache
Desktop.ini                   # Windows desktop settings
